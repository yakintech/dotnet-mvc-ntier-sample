@{
    Layout = "~/Views/Shared/Admin/AdminLayout.cshtml";
}

@model SiemensECommerce.UI.Models.VM.WebUserVM

    <form action="/WebUser/Add" method = "post" enctype = "multipart/form-data">

          @Html.ValidationSummary()

    <div class="form-group">
        <label>Name:</label>
        @Html.TextBoxFor(q => q.Name, new {@class = "form-control"})
    </div>

    <div class="form-group">
        <label>Password:</label>
        @Html.PasswordFor(q => q.Password, new {@class = "form-control"})
    </div>

        <div class="form-group">
        <label>Password:</label>
        @Html.PasswordFor(q => q.ConfirmPassword, new {@class = "form-control"})
    </div>

    <div>
        <button>Add</button>
    </div>

     
    <div>
        <button class="btn btn-primary">Add</button>
    </div>

    </form>