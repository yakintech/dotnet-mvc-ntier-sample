@{
    Layout = "~/Views/Shared/Admin/AdminLayout.cshtml";
}

@model List<Order>

<div class="table-responsive">
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">

        <thead>
            <tr>
                <th>Id</th>
                <th>Adress</th>
                <th>Phone</th>
                <th>TotalPrice</th>
                <th>FirstName</th>
                <th>LastName</th>     
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Adress</td>
                    <td>@item.Phone</td>
                    <td>@item.TotalPrice</td>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.EMail</td>
                </tr>
            }

        </tbody>


    </table>

</div>