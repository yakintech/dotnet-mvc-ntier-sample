@{
    Layout = "~/Views/Shared/Admin/AdminLayout.cshtml";
}

@model List<WebUser>

   <form asp-action="Index" method="get">
    <p>
        Search: <input type="text"/>
        <input type="submit" value="Search a WebUser" />
    </p>
</form>
<div class="table-responsive">
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">

        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>SurName</th>
                <th>Email</th>
                <th>PhoneNumber</th>
                <th>Update</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.SurName</td>
                    <td>@item.Email</td>
                    <td>@item.PhoneNumber</td>
                    <td><a href="/WebUser/Update/@item.Id">Click to Update</a></td>
                    <td><a href="/WebUser/Delete/@item.Id">Click to Delete</a></td>
                </tr>
            }

        </tbody>


    </table>

</div>